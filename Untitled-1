function oddNumbers(){



    var a[] = {1,2,3,4,5,6} //
    var b;

    var j = 0;

    for(var i =0; i<a.length; i++){
    if(a[i]%2!=0){  
        b[j] = a[i];
    }
    }
    return b;  // array with odd numbers
}

int c = oddNumbers();

